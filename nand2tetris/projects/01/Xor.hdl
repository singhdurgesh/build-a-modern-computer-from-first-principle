/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=NotA);
    Not(in=b, out=NotB);
    And(a=a, b=NotB, out=AndANotB);
    And(a=NotA, b=b, out=AndNotAB);
    Or(a=AndANotB, b=AndNotAB, out=out);
}